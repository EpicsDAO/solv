{
  "name": "@epics-dao/solv",
  "version": "3.0.7",
  "description": "Solana Validator CLI SOLV",
  "main": "dist/index.js",
  "repository": "https://github.com/EpicsDAO/solv",
  "keywords": [
    "solana",
    "validator",
    "cli",
    "validator-tool",
    "Node.js",
    "web3"
  ],
  "author": "ELSOUL LABO B.V.",
  "private": false,
  "license": "Apache-2.0",
  "bin": {
    "solv": "./dist/index.js"
  },
  "scripts": {
    "test": "jest --coverage=false --detectOpenHandles --maxWorkers=1",
    "update:packages": "ncu -u && yarn",
    "build": "npx ts-node build.ts && npx ts-node add-line.ts",
    "publish": "npm publish",
    "solv:build": "cd solv-debian/debian && debuild -us -uc",
    "solv:pro": "cd solv-debian/debian && debuild -S -sa",
    "solv:sign": "debsign solv_*.changes",
    "dev": "nodemon",
    "start": "node dist/index.js",
    "discord:epics": "npx ts-node -r tsconfig-paths/register --transpile-only ./.github/workflows/runDiscordChangeLog.ts epics"
  },
  "dependencies": {
    "@skeet-framework/utils": "1.2.4",
    "chalk": "5.3.0",
    "chalk-pipe": "6.0.0",
    "cli-progress": "3.12.0",
    "cli-spinner": "0.2.10",
    "cli-table3": "0.6.3",
    "commander": "11.1.0",
    "dotenv": "16.3.1",
    "dotenv-cli": "7.3.0",
    "inquirer": "9.2.12",
    "jest": "29.7.0",
    "node-cron": "3.0.3",
    "node-fetch": "3.3.2",
    "prompt": "1.3.0"
  },
  "devDependencies": {
    "@skeet-framework/discord-utils": "^0.2.14",
    "@types/cli-progress": "3.11.5",
    "@types/cli-spinner": "0.2.3",
    "@types/inquirer": "9.0.7",
    "@types/jest": "29.5.11",
    "@types/node": "20.11.5",
    "@types/node-cron": "^3.0.11",
    "@types/node-fetch": "^2.6.11",
    "@types/prompt": "1.1.8",
    "babel-loader": "9.1.3",
    "esbuild": "0.19.11",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "nodemon": "3.0.3",
    "npm-check-updates": "16.14.12",
    "prettier": "3.2.4",
    "ts-jest": "29.1.1",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.3.3"
  }
}